<h2>All Celebrities</h2>

<ul>
  {{#each celebrity}}
  <li>
    <p>Name: {{name}}</p>
    
    <a href="/celebrities/{{_id}}">See details</a>
    
    <form action="/celebrities/{{_id}}/edit" method="get">
      <button type="submit">Edit</button>
    </form>
    
    <form action="/celebrities/{{_id}}/delete" method="post">
      <button type="submit">Delete</button>
    </form>
  </li>
  {{/each}}
</ul>

<a href="/">Home</a>