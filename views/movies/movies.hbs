<h2>All Movies</h2>

  {{#each movie}}
  <div class ='list-container'>
    <h5>{{title}}</h5>
    
    <a href="/movies/{{_id}}">See details</a>
    
    <form action="/movies/{{_id}}/edit" method="get">
    <button type="submit">Edit</button>
    </form>
    
    <form action="/movies/{{_id}}/delete" method="post">
    <button type="submit">Delete</button>
    </form>
  </div>
  {{/each}}

<a href="/">Home</a>